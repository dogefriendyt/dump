Tab1Section:NewButton("Automatic cooking on ", "If you press this button you will automatically cook really fast", function()
 game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "Credits";
                Text = "Exo Hub";
                Duration = 1.5;
            })
local Cooking = game.Players.LocalPlayer.PlayerScripts.CookingNew local CookProgress = require(Cooking.CookProgress) local MultiClick = require(Cooking.InputDetectors.MultiClick) local MouseMovement = require(Cooking.InputDetectors.MouseMovement) local MouseSpin = require(Cooking.InputDetectors.MouseSpin) local run = CookProgress.run CookProgress.run = function(...) local ARGS = {...} ARGS[3] = 0 return run(unpack(ARGS)) end MultiClick.start = function(...) local ARGS = {...} ARGS[3]() end MouseMovement.start = function(...) local ARGS = {...} ARGS[3]() end MouseSpin.start = function(...) local ARGS = {...} ARGS[3]() end

end)

